{
  "env": {
    "jest": true
  },
  "root": true,
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "@typescript-eslint/parser"
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "extends": [
    "plugin:vue/base",
    "plugin:prettier/recommended",
    // "plugin:vue/vue3-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "comma-dangle": "off"
  }
}
